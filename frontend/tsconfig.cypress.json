{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "types": ["cypress"],
    "module": "ESNext",
    "target": "ES2020",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "allowJs": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "noEmit": true
  },
  "include": [
    "cypress.config.ts",
    "cypress/**/*.ts",
    "cypress/**/*.tsx"
  ]
}
